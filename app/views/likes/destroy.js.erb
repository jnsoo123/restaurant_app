<% if @posts.present? %>
  $('#announcement_area').html("<%= j render('posts/announcement', posts: @posts) %>")
<% else %>
  <% if @user.present? %>
    $('#user_reviews_area').html("<%= j render('users/reviews', user: @user) %>")
  <% else %>
    $('#reviews_area').html("<%= j render('ratings/rating', restaurant: @restaurant) %>")
  <% end %>
<% end %>