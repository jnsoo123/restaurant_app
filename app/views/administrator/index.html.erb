<table class = 'table'>
	<th>Username</th>
	<th>Name</th>
	<th>Email</th>
	<th>Location</th>
	<th>Profile Picture URL</th>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username%></td>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.location %></td>
		<td><%= user.profile_picture_url %></td>
		<td><%= link_to 'Edit', edit_user_path(user.id), method: :get%></td>
	</tr>
<% end%>
</table>